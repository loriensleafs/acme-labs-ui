{
	"name": "@acme-labs/ui",
	"version": "1.0.0",
	"license": "MIT",
	"sideEffects": false,
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"typings": "dist/types/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"default": "./dist/esm/index.js"
		}
	},
	"scripts": {
		"build": "concurrently yarn:build:*",
		"build:esm": "cross-env BABEL_ENV=esm babel src --root-mode upward --extensions .ts,.tsx -d dist/esm --source-maps",
		"build:cjs": "cross-env BABEL_ENV=cjs babel src --root-mode upward --extensions .ts,.tsx -d dist/cjs --source-maps",
		"build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
		"clean": "rimraf dist && rimraf node_modules",
		"prebuild": "rimraf dist",
		"start": "nodemon --watch src --exec yarn build -e ts,tsx",
		"test": "jest --env=jsdom --passWithNoTests",
		"test:cov": "yarn test --coverage",
		"version": "yarn build"
	},
	"dependencies": {
		"@acme-labs/accordion": "^1.0.0",
		"@acme-labs/alert": "^1.0.0",
		"@acme-labs/avatar": "^1.0.0",
		"@acme-labs/breadcrumb": "^1.0.0",
		"@acme-labs/button": "^1.0.0",
		"@acme-labs/checkbox": "^1.0.0",
		"@acme-labs/clickable": "^1.0.0",
		"@acme-labs/close-button": "^1.0.0",
		"@acme-labs/control-box": "^1.0.0",
		"@acme-labs/counter": "^1.0.0",
		"@acme-labs/css-reset": "^1.0.0",
		"@acme-labs/editable": "^1.0.0",
		"@acme-labs/form-control": "^1.0.0",
		"@acme-labs/hooks": "^1.0.0",
		"@acme-labs/icon": "^1.0.0",
		"@acme-labs/image": "^1.0.0",
		"@acme-labs/input": "^1.0.0",
		"@acme-labs/layout": "^1.0.0",
		"@acme-labs/menu": "^1.0.0",
		"@acme-labs/modal": "^1.0.0",
		"@acme-labs/number-input": "^1.0.0",
		"@acme-labs/pin-input": "^1.0.0",
		"@acme-labs/popover": "^1.0.0",
		"@acme-labs/portal": "^1.0.0",
		"@acme-labs/progress": "^1.0.0",
		"@acme-labs/radio": "^1.0.0",
		"@acme-labs/select": "^1.0.0",
		"@acme-labs/skeleton": "^1.0.0",
		"@acme-labs/slider": "^1.0.0",
		"@acme-labs/spinner": "^2.0.0",
		"@acme-labs/stat": "^1.0.0",
		"@acme-labs/switch": "^1.0.0",
		"@acme-labs/system": "^1.0.0",
		"@acme-labs/table": "^1.0.0",
		"@acme-labs/tabs": "^1.0.0",
		"@acme-labs/tag": "^1.0.0",
		"@acme-labs/textarea": "^1.0.0",
		"@acme-labs/theme": "^1.0.0",
		"@acme-labs/toast": "^1.0.0",
		"@acme-labs/tooltip": "^1.0.0",
		"@acme-labs/transition": "^1.0.0",
		"@acme-labs/visually-hidden": "^1.0.0",
		"@chakra-ui/react": "^1.6.1",
		"@chakra-ui/react-env": "^1.0.4",
		"@chakra-ui/system": "^1.6.5",
		"@chakra-ui/theme": "^1.8.5",
		"@chakra-ui/utils": "^1.8.0",
		"react": "^17.0.2"
	}
}
